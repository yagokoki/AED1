#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void Insercao(int n, int v[]) {
  int i, j, x;
  for (j = 1; /*A*/ j < n; j++) {
    x = v[j];
    for (i = j - 1; i >= 0 && v[i] > x; i--)
      v[i + 1] = v[i];
    v[i + 1] = x;
  }
}

int main(void) {
  int n, i, j, k;
  clock_t tinicial, tfinal;
  double tempo;
  double tempomili;

  int v[20000];

  for (i = 1; i <= 4; i++) {
    n = 5000 * i;
    for (j = 0; j < n; j++) {
      v[j] = rand();
    }
    tinicial = clock();
    Insercao(n, v);
    tfinal = clock();

    tempo = ((double)(tfinal - tinicial)) / CLOCKS_PER_SEC;
    tempomili = tempo * 1000;
    printf("\nquantidade de valores ordenados: %d\n", n);
    printf("tempo de execucao: %f milissegundos ", tempomili);
  }
  return 0;
}
